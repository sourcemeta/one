{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$vocabulary": {
    "https://json-schema.org/draft/2020-12/vocab/core": true,
    "https://json-schema.org/draft/2020-12/vocab/applicator": true,
    "https://json-schema.org/draft/2020-12/vocab/unevaluated": true,
    "https://json-schema.org/draft/2020-12/vocab/validation": true,
    "https://json-schema.org/draft/2020-12/vocab/meta-data": true,
    "https://json-schema.org/draft/2020-12/vocab/format-annotation": true,
    "https://json-schema.org/draft/2020-12/vocab/content": true,
    "https://json-schema.org/extension/database/v1": false
  },
  "$dynamicAnchor": "meta",
  "title": "JSON Schema Extensions Meta-Schema",
  "description": "A meta-schema combining all JSON Schema extensions in this registry",
  "examples": [
    {
      "type": "object",
      "properties": {
        "name": {
          "type": "string"
        }
      }
    },
    {
      "type": "object",
      "sqlObjectName": "EMPLOYEES",
      "properties": {
        "hire_date": {
          "extendedType": "date"
        }
      }
    },
    {
      "extendedType": "number",
      "sqlPrecision": 10,
      "sqlScale": 2
    }
  ],
  "x-documentation": "https://github.com/sourcemeta-research/vocabularies",
  "allOf": [
    {
      "$ref": "https://json-schema.org/draft/2020-12/schema"
    },
    {
      "$ref": "./database/v1.json"
    }
  ]
}
