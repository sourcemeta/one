{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "Sourcemeta One Collection",
  "description": "Represents a set of schemas to be mounted into the One",
  "examples": [
    {
      "title": "My Schema Collection",
      "description": "A collection of JSON Schemas",
      "path": "schemas"
    }
  ],
  "x-lint-exclude": [ "simple_properties_identifiers" ],
  "type": "object",
  "properties": {
    "title": {
      "type": "string"
    },
    "description": {
      "type": "string"
    },
    "x-sourcemeta-one:alert": {
      "type": "string"
    },
    "x-sourcemeta-one:evaluate": {
      "type": "boolean"
    },
    "x-sourcemeta-one:provenance": {
      "$ref": "./rpath.json"
    },
    "baseUri": {
      "$ref": "https://schemas.sourcemeta.com/sourcemeta/std/v0/ietf/uri/uri-reference"
    },
    "defaultDialect": {
      "$ref": "https://schemas.sourcemeta.com/sourcemeta/std/v0/ietf/uri/uri"
    },
    "email": {
      "$ref": "https://schemas.sourcemeta.com/sourcemeta/std/v0/ietf/email/address"
    },
    "github": {
      "type": "string"
    },
    "path": {
      "$ref": "https://schemas.sourcemeta.com/sourcemeta/std/v0/ieee/posix/2017/path"
    },
    "resolve": {
      "type": "object",
      "propertyNames": {
        "$ref": "https://schemas.sourcemeta.com/sourcemeta/std/v0/ietf/uri/uri-reference"
      },
      "additionalProperties": {
        "$ref": "https://schemas.sourcemeta.com/sourcemeta/std/v0/ietf/uri/uri-reference"
      }
    },
    "website": {
      "$ref": "https://schemas.sourcemeta.com/sourcemeta/std/v0/ietf/uri/url"
    }
  },
  "additionalProperties": false
}
