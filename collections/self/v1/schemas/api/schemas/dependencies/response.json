{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "Sourcemeta One Schema Dependencies API Response",
  "description": "The response format for the schema dependencies API endpoint",
  "examples": [
    [
      {
        "from": "https://example.com/schemas/user.json",
        "to": "https://example.com/schemas/address.json",
        "at": "/properties/address"
      }
    ]
  ],
  "type": "array",
  "items": {
    "type": "object",
    "required": [ "from", "to", "at" ],
    "properties": {
      "at": {
        "$ref": "https://schemas.sourcemeta.com/sourcemeta/std/v0/ietf/jsonpointer/pointer"
      },
      "from": {
        "$ref": "https://schemas.sourcemeta.com/sourcemeta/std/v0/ietf/uri/url"
      },
      "to": {
        "$ref": "https://schemas.sourcemeta.com/sourcemeta/std/v0/ietf/uri/url"
      }
    },
    "additionalProperties": false
  }
}
